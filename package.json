  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.16.0"
  },
  "scripts": {
    "build-prod": "cross-env NODE_ENV=production webpack --display-error-details -p --config webpack.config.js",
    "build:server:prod": "cross-env NODE_ENV=production webpack --display-error-details --config webpack.config.server.js",
    "server:migrate": "cross-env MIGRATE_ONLY=true node dist/server.js",
    "build": "webpack --display-error-details -p --config webpack.config.js",
    "build-prod-report": "cross-env NODE_ENV=production BUILD_REPORT=true webpack --display-error-details -p --config webpack.config.js",
    "build:watch": "webpack --display-error-details --config webpack.config.js --watch",
    "watch": "npm run build:server:dev && npm-run-all --parallel build:watch nodemon:prod watch:server",
    "create-migration": "./server/db/migration/create-migration.sh",
    "build:server:dev": "cross-env NODE_ENV=development webpack --display-error-details --config webpack.config.server.js",
    "dev:server": "npm run build:server:dev && npm-run-all --parallel nodemon:prod watch:server",
    "watch:server": "cross-env NODE_ENV=development webpack --display-error-details --config webpack.config.server.js --watch",
    "nodemon:prod": "cross-env NODE_ENV=development node-dev --no-notify --inspect -- dist/server.js",
    "test": "mocha-webpack --timeout 30000 --display-error-details --webpack-config test/webpack.config.test.js \"test/*/*.js\"",
    "test:docker": "test/run_docker_tests.sh",
    "postinstall": "webpack --display-error-details --config webpack.config.js",
    "preversion": "git diff --quiet || { echo \"Working directory is dirty\"; exit 1; }; yarn test",
    "postversion": "git push --tags && git push && echo \"Successfully released version $npm_package_version!\""
  },

  "scripts": {
    "build-dev": "cross-env NODE_ENV=development webpack --mode development --config webpack.config.babel.js",
    "build-prod-report": "cross-env NODE_ENV=production BUILD_REPORT=true webpack --mode production --config webpack.config.babel.js",
    "build-prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.config.babel.js",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config webpack.config.server.babel.js",
    "build:server:prod": "cross-env NODE_ENV=production webpack  --config webpack.config.server.babel.js",
    "build:watch": "npm run build-dev --watch",
    "client:dev-server": "NODE_ENV=development webpack serve",
    "build": "run-s build:server:prod build-prod",
    "clean": "rimraf dist/** ./node_modules/.cache/",
    "dev:server": "npm run build:server:dev && npm-run-all --parallel dev:server:nodemon watch:server",
    "dev:server:nodemon": "cross-env NODE_ENV=development node-dev --no-notify --inspect -- dist/server.js",
    "build:test:integration": "webpack --config test/integration/config/webpack.config.js",
    "build:test:unit": "webpack --config test/unit/config/webpack.config.js",
    "heroku-prebuild": "echo \"//npm.pkg.github.com/:_authToken=${NPM_GITHUB_TOKEN}\" >> .npmrc",
    "jest:integration": "jest dist/__tests__/bundle.integration.js",
    "jest:unit": "jest  dist/__tests__/bundle.unit.js",
    "test:integration": "run-s build:test:integration jest:integration",
    "test:unit": "run-s build:test:unit jest:unit",
    "test:e2e": "jest --config=test/e2e/jest.config.js",
    "test:e2e:codegen": "npx playwright --save-storage=test/e2e/resources/auth.json codegen http://localhost:9090",
    "test:e2e:watch": "cross-env PWDEBUG=1 jest --config=test/e2e/jest.config.js",
    "test": "run-s test:unit test:e2e",
    "test:docker": "test/bin/testExecDocker.sh",
    "preversion": "git diff --quiet || { echo \"Working directory is dirty\"; exit 1; }; yarn test:docker",
    "postversion": "git push --tags && git push && echo \"Successfully released version $npm_package_version!\"",
    "prod": "run-s clean build start",
    "server:migrate": "cross-env MIGRATE_ONLY=true node dist/server.js",
    "watch:server": "cross-env NODE_ENV=development webpack --config webpack.config.server.babel.js --watch",
    "watch": "npm run build:server:dev && npm-run-all --parallel client:dev-server dev:server:nodemon watch:server",
    "start": "node dist/server.js",
    "prepare": "husky install"
  },
